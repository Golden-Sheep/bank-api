{
    "name": "golden-sheep/bank-api",
    "type": "project",
    "description": "API BANK",
    "keywords": ["laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "geekcom/validator-docs": "^3.9",
        "guzzlehttp/guzzle": "^7.8.1",
        "laravel/framework": "^10.43.0",
        "laravel/sanctum": "^3.3.3",
        "laravel/tinker": "^2.9.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.7",
        "larastan/larastan": "^2.6",
        "phpmd/phpmd": "^2.11",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "pint-fix": "./vendor/bin/pint -v",
        "pint-test": "./vendor/bin/pint --test",
        "test-codequality": "./vendor/bin/phpmd ./app text phpmd-ics",
        "phpmd": "./vendor/bin/phpmd app text phpmd-laravel",
        "phpmd-ics": "./vendor/bin/phpmd app text phpmd-ics",
        "phpstan": "./vendor/bin/phpstan analyse --memory-limit=-2G",
        "phpunit-test": "@php artisan test",
        "phpunit-coverage": "XDEBUG_MODE=coverage php artisan test --coverage --min=38",
        "pipelines": "XDEBUG_MODE=off composer pint-test && composer test-codequality && composer phpstan && composer phpunit-test"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
